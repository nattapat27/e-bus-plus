<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="home.HomeController">
   <children>
      <AnchorPane prefHeight="100.0" prefWidth="812.0">
         <children>
            <Button fx:id="homeButton" layoutX="48.0" layoutY="26.0" mnemonicParsing="false" onAction="#handleHomeAction" prefHeight="50.0" prefWidth="130.0" text="Refresh" AnchorPane.leftAnchor="48.0">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/refresh.png" />
                     </image>
                  </ImageView>
               </graphic>
               <font>
                  <Font size="18.0" />
               </font></Button>
            <Button fx:id="reportButton" layoutX="209.0" layoutY="26.0" mnemonicParsing="false" onAction="#handleReportAction" prefHeight="50.0" prefWidth="130.0" text="Report" AnchorPane.leftAnchor="209.0">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/ProblemImage.png" />
                     </image>
                  </ImageView>
               </graphic>
               <font>
                  <Font size="18.0" />
               </font></Button>
            <Button fx:id="logoutButton" layoutX="623.0" layoutY="26.0" mnemonicParsing="false" onAction="#handleLogoutAction" prefHeight="50.0" prefWidth="130.0" text="Logout" AnchorPane.rightAnchor="47.0">
               <graphic>
                  <ImageView fitHeight="35.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/LoginImage.png" />
                     </image></ImageView>
               </graphic>
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <TableView fx:id="table" layoutX="50.0" layoutY="154.0" prefHeight="421.0" prefWidth="735.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0">
      </TableView>
      <Label layoutX="50.0" layoutY="112.0" text="Problem" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
